{
    "name": "ash",
    "displayName": "KoLmafia ASH",
    "description": "Language support for KoLmafia ASH scripts.",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ash",
            "aliases": ["KoLMafia ASH", "ash"],
            "extensions": [".ash"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ash",
            "scopeName": "source.ash",
            "path": "./syntaxes/ash.tmLanguage.json"
        }]
    }
}